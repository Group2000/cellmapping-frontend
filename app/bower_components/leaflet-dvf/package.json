    "data visualization"
  ],
  "maintainers": [
    {
      "name": "HumanGeo"
    }
  ],
  "devDependencies": {
    "leaflet": "~0.7.3",
    "grunt": "~0.4.5",
    "grunt-cli": "~0.1.13",
    "grunt-contrib-uglify": "~0.7.0",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-contrib-concat": "~0.5.0"
  },
  "main": "dist/leaflet-dvf.js",
  "style": "dist/css/dvf.css",
  "repository": {
  	"type": "git",
	"url": "https://github.com/humangeo/leaflet-dvf.git"
